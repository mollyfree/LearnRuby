strings = ["HTML", "CSS", "JavaScript", "Python", "Ruby"]

# Add your code below!

symbols = [ ]

strings.each do |s|
        s = s.to_sym
    symbols.push(s)
end

strings = ["HTML", "CSS", "JavaScript", "Python", "Ruby"]

# Add your code below!

symbols = [ ]

strings.each do |s|
        s = s.intern
    symbols.push(s)
end

